{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "jsx": "react-jsx",
    "strict": true,
    "paths": {
      "@firebase/auth": ["./node_modules/@firebase/auth/dist/index.rn.d.ts"],
      "@/*": ["./*"]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "FirebaseConfig.js",
    "app/(tabs)/index.jsx",
    "app/_layout.jsx"
, "app/(tabs)/two.jsx", "app/(tabs)/_layout.jsx"  ]
}
